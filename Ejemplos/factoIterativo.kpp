int factoIterativo(int f) {
    int i, acum;

    if (f > -1) {
        i = 2;
        acum = 1;

        while(i < f + 1) {
            acum = acum * i;
            i = i + 1;
        }
    }
    else {
        acum = -1;
    }

    return acum;
}

main {
    int j, w;
    j = input(int);

    w = factoIterativo(j);

    print(w);
}